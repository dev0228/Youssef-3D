{
  "expo": {
    "name": "arveesio",
    "description": "Arveesio application for 3D",
    "slug": "arveesio",
    "privacy": "unlisted",
    "platforms": ["ios", "android"],
    "version": "1.8.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/Onboarding.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    },
    "android": {
      "package": "com.eleclet.arveesio"
    }
  },
  "plugins": ["@viro-community/react-viro"]
}
